<h1>Select your Platforms<h1>


<%= form_for @system_user do |f| %>
    <%= f.label "Select your account:"%>
    <%= f.collection_select  :user_id, User.all, :id, :name %>

    <%= f.label "Add a system: " %>
    <%= f.collection_select :system_id, System.all, :id, :name %>

    <%= f.submit %>
<% end %>
