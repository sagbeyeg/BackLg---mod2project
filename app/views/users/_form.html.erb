<%= form_for @user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :password %>
    <%= f.text_field :password %>

    <%= f.label :age %>
    <%= f.number_field :age %>

    <%= f.label :interest %>
    <%= f.text_area :interest %>

    <%=f.submit%>
<% end%>